{
  "name": "rentguy-pwa-scanner",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier -w .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "ci:type-sanity": "ts-prune --ignore @core,@domain,@infra,@ui",
    "dep:graph": "dependency-cruiser -c .dependency-cruiser.cjs src",
    "complexity": "madge --circular --extensions ts,tsx,js,jsx src",
    "dup": "jscpd --min-lines 5 --reporters console --format verbose src backend/app",
    "test": "vitest run --coverage --passWithNoTests",
    "quality:report": "node scripts/quality-report.mjs",
    "quality:all": "npm run lint && npm run typecheck && npm run dup && npm run complexity && npm run test && npm run quality:report",
    "test:e2e": "playwright test -c tests/e2e/playwright.config.ts"
  },
  "dependencies": {
    "@zxing/browser": "^0.1.5",
    "axios": "^1.7.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.9.2",
    "@playwright/test": "^1.47.2",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.4",
    "dependency-cruiser": "^16.6.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^1.0.3",
    "eslint-plugin-testing-library": "^6.3.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "husky": "^9.1.6",
    "jscpd": "^3.5.10",
    "lint-staged": "^15.2.10",
    "madge": "^7.0.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.11.4",
    "ts-unused-exports": "^10.1.0",
    "typescript-eslint": "^8.10.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vitest": "^2.1.4"
  }
}
